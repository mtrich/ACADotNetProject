@model CreateJobViewModel

<h1>CreateJob</h1>

<form asp-action="createJob" method="post">
    <div class="form-group">
        <label asp-for="JobListing.TypeOfJob"></label>
        <input class="form-control" asp-for="JobListing.TypeOfJob" />
    </div>
    <div class="form-group">
        <label asp-for="Organization.Address"></label>
        <input class="form-control" asp-for="Organization.Address" />
    </div>
    <div class="form-group">
        <label asp-for="Organization.City"></label>
        <input class="form-control" asp-for="Organization.City" />
    </div>
    <div class="form-group">
        <label asp-for="Organization.State"></label>
        <input class="form-control" asp-for="Organization.State" />
    </div>
    <div class="form-group">
        <label asp-for="Organization.Email"></label>
        <input class="form-control" asp-for="Organization.Email" />
    </div>
    <div class="form-group">
        <label asp-for="Organization.PhoneNumber"></label>
        <input class="form-control" asp-for="Organization.PhoneNumber" />
    </div>
    <div class="form-group">
        <label asp-for="Image"></label>
        <input type="file" accept=".jpg" class="form-control-file" asp-for="Image" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>