@model Organization
@{
    ViewData["Title"] = "Index";
}

<h1>Profile</h1>
<p>This page is only visible to Organization Admins</p>

<td>@Html.ActionLink("Edit Organization", "EditOrg", new { id = Model.Id }, new { @class = "btn btn-primary" })</td>
<table class="table table-dark">
    <tr>
        <th>Logo</th>
        <th>Company Name</th>
        <th>Address</th>
        <th>City</th>
        <th>State</th>
        <th>Email</th>
        <th>PhoneNumber</th>

    </tr>

    <tr>
        <td>
            <img src="@Model.LogoImageURL" alt="company Logo" class="logo">
        </td>
        <td>@Model.CompanyName</td>
        <td>@Model.Address</td>
        <td>@Model.City</td>
        <td>@Model.State</td>
        <td>@Model.Email</td>
        <td>@Model.PhoneNumber</td>

    </tr>
</table>